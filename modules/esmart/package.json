{
  "name": "react-native-esmart",
  "version": "0.0.0",
  "description": "Esmart module",
  "main": "./lib/commonjs/index.js",
  "author": " <> ()",
  "license": "UNLICENSED",
  "homepage": "#readme",
  "create-react-native-library": {
    "type": "module-legacy",
    "languages": "kotlin-objc",
    "version": "0.41.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "node-fetch": "^3.3.2",
    "react-native-builder-bob": "^0.30.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "source": "./src/index.tsx",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "scripts": {
    "download-libs": "dotenv -- tsx download-libs.js",
    "build": "bob build",
    "prepare": "bob build"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "windows",
    "cpp",
    "react-native-esmart.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
